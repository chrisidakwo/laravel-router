{
    "name": "sebastiaanluca/laravel-router",
    "type": "library",
    "description": "An organized approach to handling Laravel routes. Provides additional functionality on top of the default HTTP router.",
    "keywords": [
        "Laravel",
        "router",
        "laravel-router",
        "http"
    ],
    "homepage": "https://github.com/sebastiaanluca/laravel-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "https://www.sebastiaanluca.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7",
        "laravel/framework": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.7",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|^3.4.2",
        "phpunit/phpunit": "^5.7|^6"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\Router\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
