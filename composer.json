{
    "name": "sebastiaanluca/laravel-router",
    "type": "library",
    "description": "An organized approach to handling Laravel routes. Provides additional functionality on top of the default HTTP router.",
    "keywords": [
        "Laravel",
        "router",
        "laravel-router",
        "http"
    ],
    "homepage": "https://github.com/sebastiaanluca/laravel-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Luca",
            "email": "hello@sebastiaanluca.com",
            "homepage": "http://www.sebastiaanluca.com",
            "role": "author"
        }
    ],
    "require": {
        "php": "^5.6.4|^7.0",
        "laravel/framework": "~5.3.0",
        "illuminate/support": "~5.3.0",
        "illuminate/container": "~5.3.0",
        "illuminate/contracts": "~5.3.0",
        "illuminate/routing": "~5.3.0",
        "illuminate/events": "~5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "SebastiaanLuca\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastiaanLuca\\Router\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
